{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es2015",
      "esnext"
    ],
    "baseUrl": "../..",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "include": ["../../@webappDirectory@/**/*", "../generatedTs/**/*"],
  "exclude": ["node_modules"]
}
